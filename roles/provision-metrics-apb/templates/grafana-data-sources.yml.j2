datasources:
- name: Prometheus
  # <string, required> datasource type. Required
  type: prometheus
  # <string, required> access mode. direct or proxy. Required
  access: proxy
  # <string> url
  url: http://prometheus-internal
  # <bool> mark as default datasource. Max one per org
  is_default: true
  version: 1
  editable: true
  json_data: {"timeInterval": "5s"}
- name: Postgres
  type: postgres
  access: proxy
  url: {{ postgres_service_name }}:5432
  user: "{{ postgres_user }}"
  secure_json_data:
    password: {{ postgres_password }}
  database: "{{ postgres_database }}"
  version: 1
  editable: true
  json_data:
    sslmode: "disable"